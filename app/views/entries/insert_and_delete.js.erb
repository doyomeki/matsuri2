alert(<%= @status %>)
if (<%= @status %>) {
  <% if @other_entries %>
    <% @other_entries.each do |entry| %>
      $('#join_button_<%= entry.content_id %> ').text("不参加");
    <% end %>
  <% end %>
  $('#join_button_<%= @content.id %> ').text("参加");
} else {
  $('#join_button_<%= @content.id %> ').text("不参加");
}
